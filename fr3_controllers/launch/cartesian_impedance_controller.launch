<?xml version="1.0" ?>
<launch>
  <!-- Args -->
  <arg name="robot" default="fr3"/>
  <arg name="arm_id" default="$(arg robot)"/>
  <arg name="robot_ip" default="192.168.1.11"/>
  <arg name="load_gripper" default="true"/>
  <arg name="tf_prefix" default="$(arg arm_id)" />

  <!-- Franka control -->
  <include file="$(find franka_control)/launch/franka_control.launch">
    <arg name="robot_ip" value="$(arg robot_ip)" />
    <arg name="arm_id" value="$(arg arm_id)" />
    <arg name="load_gripper" value="$(arg load_gripper)" />
    <arg name="tf_prefix" value="$(arg tf_prefix)" />
  </include>

  <!-- Controller parameters -->
  <rosparam command="load" file="$(find fr3_controllers)/config/fr3_controllers.yaml" subst_value="true" />

  <!-- Controller spawner -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="cartesian_impedance_controller"/>

</launch>
